                <div class="panel panel-default">
                  <div class="panel-heading">
                    <h4 class="panel-title">
                      <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                        Active Blogs
                      </a>
                    </h4>
                  </div>
                  <div id="collapseOne" class="panel-collapse collapse in">
                    <div class="panel-body">
                      <ul class="list-unstyled list-channels">
                        <TMPL_LOOP Channels>
                          <TMPL_UNLESS message>
                            <li>
                              <a href="<TMPL_VAR url ESCAPE="HTML">" title="subscribe">
                                <i class="fa fa-rss-square orange" aria-hidden="true"></i>
                              </a>
                              <a <TMPL_IF link>href="<TMPL_VAR link ESCAPE="HTML">" </TMPL_IF>title="Open <TMPL_VAR title_plain ESCAPE="HTML">." target="_blank"><TMPL_VAR name></a>
                            </li>
                          </TMPL_UNLESS>
                        </TMPL_LOOP>
                      </ul>
                    </div>
                  </div>
                </div>
                <div class="panel panel-default">
                  <div class="panel-heading">
                    <h4 class="panel-title">
                      <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                      Inactive Blogs
                      </a>
                    </h4>
                  </div>
                  <div id="collapseTwo" class="panel-collapse collapse">
                    <div class="panel-body">
                      <ul class="list-unstyled list-inline list-channels">
                        <TMPL_LOOP Channels>
                          <TMPL_IF message>
                            <li>
                              <a href="<TMPL_VAR url ESCAPE="HTML">" title="<TMPL_VAR message ESCAPE="HTML">">
                                <img src="images/feed-inactive-icon-10x10.png" alt="(inactive feed)" />
                              </a>
                              <a <TMPL_IF link>href="<TMPL_VAR link ESCAPE="HTML">" </TMPL_IF>title="<TMPL_VAR message ESCAPE="HTML">" target="_blank">
                              <TMPL_VAR name>
                              </a>
                            </li>
                          </TMPL_IF>
                        </TMPL_LOOP>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.row-fluid -->
          </div>
